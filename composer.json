{
	"name": "v12mike/imageredirect",
	"type": "phpbb-extension",
	"description": "Rewrites extenal image URLs to point to a local file or proxy server.",
	"homepage": "https://www.phpbb.com/community/viewtopic.php?f=456&t=2487136",
	"version": "2.0.0-rc1",
	"time": "2018-10-07 12:00:00",
	"license": "GPL-2.0-only",
	"authors": [
		{
			"name": "Mike Morrin",
			"email": "v12mike@ingenious.co.nz",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=5.3.3",
		"composer/installers": "~1.0"
	},
	"extra": {
		"display-name": "Image Redirect",
		"soft-require": {
			"phpbb/phpbb": ">=3.2.0,<3.3.0@dev"
		},
		"version-check": {
			"host": "frenchcarforum.co.uk",
			"directory": "/versions",
			"filename": "imageredirect.json"
		}
	}
